{
  "name": "2023-portfolio",
  "version": "1.0.0",
  "description": "Wing's 2023 portfolio",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/stylesheets:dist/stylesheets",
    "build:sass": "sass src/stylesheets:dist/stylesheets",
