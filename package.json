    "lodash": "^4.17.21",
    "markdown-it-eleventy-img": "^0.10.1",
    "netlify-plugin-cache-folder": "^0.0.8",
    "node-fetch": "^3.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "@11ty/webc": "^0.11.4",
    "eleventy-sass": "^2.2.4",
    "eslint": "^8.55.0",
    "gsap": "^3.12.3",
